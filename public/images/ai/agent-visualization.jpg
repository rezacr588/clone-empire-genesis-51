<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with dark color -->
  <rect width="800" height="600" fill="#06060A" />
  
  <!-- Abstract grid lines -->
  <g stroke="#D9D9D9" stroke-opacity="0.05" stroke-width="1">
    <line x1="0" y1="100" x2="800" y2="100" />
    <line x1="0" y1="200" x2="800" y2="200" />
    <line x1="0" y1="300" x2="800" y2="300" />
    <line x1="0" y1="400" x2="800" y2="400" />
    <line x1="0" y1="500" x2="800" y2="500" />
    
    <line x1="100" y1="0" x2="100" y2="600" />
    <line x1="200" y1="0" x2="200" y2="600" />
    <line x1="300" y1="0" x2="300" y2="600" />
    <line x1="400" y1="0" x2="400" y2="600" />
    <line x1="500" y1="0" x2="500" y2="600" />
    <line x1="600" y1="0" x2="600" y2="600" />
    <line x1="700" y1="0" x2="700" y2="600" />
  </g>
  
  <!-- Glowing accent elements -->
  <circle cx="150" cy="450" r="80" fill="#2a918f" fill-opacity="0.15" />
  <circle cx="650" cy="150" r="100" fill="#E10600" fill-opacity="0.07" />
  
  <!-- Agent visualization UI frame -->
  <rect x="150" y="100" width="500" height="400" fill="none" stroke="#44f9ff" stroke-width="2" rx="10" ry="10" opacity="0.7" />
  
  <!-- Agent digital brain visualization -->
  <g transform="translate(400, 250)">
    <!-- Neural network visualization -->
    <g>
      <!-- Nodes -->
      <circle cx="-120" cy="-60" r="8" fill="#44f9ff" opacity="0.9" />
      <circle cx="-120" cy="0" r="8" fill="#44f9ff" opacity="0.9" />
      <circle cx="-120" cy="60" r="8" fill="#44f9ff" opacity="0.9" />
      
      <circle cx="0" cy="-90" r="8" fill="#E10600" opacity="0.9" />
      <circle cx="0" cy="-30" r="8" fill="#E10600" opacity="0.9" />
      <circle cx="0" cy="30" r="8" fill="#E10600" opacity="0.9" />
      <circle cx="0" cy="90" r="8" fill="#E10600" opacity="0.9" />
      
      <circle cx="120" cy="-60" r="8" fill="#44f9ff" opacity="0.9" />
      <circle cx="120" cy="0" r="8" fill="#44f9ff" opacity="0.9" />
      <circle cx="120" cy="60" r="8" fill="#44f9ff" opacity="0.9" />
      
      <!-- Connecting lines -->
      <g stroke="#D9D9D9" stroke-opacity="0.4" stroke-width="1">
        <!-- Input to hidden connections -->
        <line x1="-120" y1="-60" x2="0" y2="-90" />
        <line x1="-120" y1="-60" x2="0" y2="-30" />
        <line x1="-120" y1="-60" x2="0" y2="30" />
        
        <line x1="-120" y1="0" x2="0" y2="-90" />
        <line x1="-120" y1="0" x2="0" y2="-30" />
        <line x1="-120" y1="0" x2="0" y2="30" />
        <line x1="-120" y1="0" x2="0" y2="90" />
        
        <line x1="-120" y1="60" x2="0" y2="-30" />
        <line x1="-120" y1="60" x2="0" y2="30" />
        <line x1="-120" y1="60" x2="0" y2="90" />
        
        <!-- Hidden to output connections -->
        <line x1="0" y1="-90" x2="120" y2="-60" />
        <line x1="0" y1="-90" x2="120" y2="0" />
        
        <line x1="0" y1="-30" x2="120" y2="-60" />
        <line x1="0" y1="-30" x2="120" y2="0" />
        <line x1="0" y1="-30" x2="120" y2="60" />
        
        <line x1="0" y1="30" x2="120" y2="-60" />
        <line x1="0" y1="30" x2="120" y2="0" />
        <line x1="0" y1="30" x2="120" y2="60" />
        
        <line x1="0" y1="90" x2="120" y2="0" />
        <line x1="0" y1="90" x2="120" y2="60" />
      </g>
      
      <!-- Animated pulses -->
      <circle cx="-120" cy="-60" r="12" fill="#44f9ff" opacity="0">
        <animate attributeName="opacity" values="0;0.5;0" dur="2s" begin="0s" repeatCount="indefinite" />
      </circle>
      <circle cx="-120" cy="0" r="12" fill="#44f9ff" opacity="0">
        <animate attributeName="opacity" values="0;0.5;0" dur="2s" begin="0.7s" repeatCount="indefinite" />
      </circle>
      <circle cx="0" cy="-30" r="12" fill="#E10600" opacity="0">
        <animate attributeName="opacity" values="0;0.5;0" dur="2s" begin="1s" repeatCount="indefinite" />
      </circle>
      <circle cx="120" cy="0" r="12" fill="#44f9ff" opacity="0">
        <animate attributeName="opacity" values="0;0.5;0" dur="2s" begin="1.5s" repeatCount="indefinite" />
      </circle>
    </g>
  </g>
  
  <!-- Data visualization panels -->
  <rect x="190" y="380" width="100" height="80" fill="#2a918f" opacity="0.2" rx="5" ry="5" />
  <rect x="350" y="380" width="100" height="80" fill="#E10600" opacity="0.2" rx="5" ry="5" />
  <rect x="510" y="380" width="100" height="80" fill="#44f9ff" opacity="0.2" rx="5" ry="5" />
  
  <!-- Data visualizations -->
  <g stroke="#44f9ff" stroke-width="1.5" fill="none">
    <polyline points="200,440 210,420 220,430 230,410 240,420 250,400 260,410 270,415 280,400" />
  </g>
  <g stroke="#E10600" stroke-width="1.5" fill="none">
    <polyline points="360,440 370,425 380,435 390,410 400,425 410,405 420,415 430,430 440,410" />
  </g>
  <g stroke="#D9D9D9" stroke-width="1.5" fill="none">
    <polyline points="520,410 530,430 540,420 550,440 560,410 570,430 580,420 590,435 600,415" />
  </g>
  
  <!-- Interface elements -->
  <rect x="200" y="120" width="400" height="30" fill="#2a918f" opacity="0.3" rx="5" ry="5" />
  <rect x="200" y="170" width="150" height="10" fill="#D9D9D9" opacity="0.3" rx="2" ry="2" />
  <rect x="200" y="190" width="120" height="10" fill="#D9D9D9" opacity="0.3" rx="2" ry="2" />
  
  <!-- Accent lines -->
  <line x1="100" y1="550" x2="700" y2="550" stroke="#2a918f" stroke-width="2" stroke-opacity="0.5" />
  <line x1="400" y1="50" x2="400" y2="550" stroke="#E10600" stroke-width="2" stroke-opacity="0.5" />
</svg>
